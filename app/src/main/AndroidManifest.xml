<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AI_Travel_Planner"
        tools:targetApi="31">

        <activity

            android:name=".ui.home.booking.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.AI_Travel_Planner">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ui.home.login.StartScreen" />
        <activity android:name=".ui.home.login.LoginScreen" />
        <activity android:name=".ui.home.login.SigninScreen" />
        <activity android:name=".ui.home.home.HomeScreen" />
        <activity android:name=".ui.home.describe.DescribeScreen" />
        <activity android:name=".ui.home.plan.PlanScreen" />
        <activity android:name=".ui.home.plan.WeatherScreen" />
        <activity android:name=".ui.home.plan.CalendarScreen" />
        <activity android:name=".ui.home.plan.GuideScreen" />
        <activity android:name=".ui.home.activity.AddItineraryScreen" />
        <activity android:name=".ui.home.friend.FriendScreen" />
        <activity android:name=".ui.home.profile.ProfileScreen" />
        <activity android:name=".ui.home.favorite.FavoriteScreen" />




    </application>
</manifest>
